{
    "name": "joomla/event",
    "type": "joomla-package",
    "description": "Joomla Event Package",
    "keywords": ["joomla", "framework", "event"],
    "homepage": "https://github.com/joomla-framework/event",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "~7.2"
    },
    "require-dev": {
        "joomla/coding-standards": "~2.0@alpha",
        "joomla/console": "~2.0",
        "phpunit/phpunit": "~8.2"
    },
    "suggest": {
        "joomla/console": "If you want to use the DebugEventDispatcherCommand class, please install joomla/console:~2.0"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Event\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Event\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
	        "dev-2.0-dev": "2.0-dev"
        }
    }
}
